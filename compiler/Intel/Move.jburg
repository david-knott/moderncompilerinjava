/**
* Rules that match tiles that move temps.
*/ 

@Include "./Move_Load.jburg"
@Include "./Move_Store.jburg"

move = MOVE(temp arg0, CONST(void) arg1) : 1
{
	emitter.moveConstToTemp(arg0, arg1);
	return null;
}

move = MOVE(temp arg0, exp arg1) : 1
{
	emitter.moveExpToTemp(arg0, arg1);
	return null;
}